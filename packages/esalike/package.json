{
  "name": "esalike",
  "version": "1.0.2",
  "description": "An esalike markdown support implemented as a NPM package",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs --dts",
    "dev": "npm run build --watch",
    "lint": "eslint 'src/**'",
    "lint:fix": "eslint --fix 'src/**/*.{ts,js}'",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/willnet-inc/esalike.git"
  },
  "keywords": [
    "esarea",
    "markdown"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/willnet-inc/esalike/issues"
  },
  "homepage": "https://github.com/willnet-inc/esalike#readme",
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^30.0.0",
    "eslint-config-custom": "*",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^30.1.2",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "tsconfig": "*",
    "tsup": "^8.5.0",
    "typescript": "^5.0.2"
  },
  "files": [
    "dist/**/*"
  ],
  "publishConfig": {
    "access": "public"
  }
}
